<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.creditinfo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name">

        </activity>
        <activity
            android:name=".ui.login.MainScreenActivity"
            android:label="CreditInfo" />

        <activity
            android:name=".ui.login.RegistrationActivity"
            android:label="Register" />

        <activity
            android:name=".ui.login.OwnerInfoActivity"
            android:label="Owner Info" />

        <activity
            android:name=".ui.login.NewLoginActivity"
            android:label="CreditInfo Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.login.AddCustomerActivity"
            android:label="Add Customer" />

        <activity android:name=".ui.login.SingleCustomerActivity"
            android:label="Customer Info">
        </activity>
        <activity android:name=".ui.login.AddCustomerTransactionActivity"
            android:label="Add Transaction">
        </activity>
    </application>

</manifest>